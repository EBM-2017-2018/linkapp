    "debug": "~2.6.0",
    "dotenv-cli": "^1.4.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.2",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.9.3",
    "mongoose-seed": "^0.4.1",
    "morgan": "^1.7.0",
    "passport": "^0.4.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.3.2",
    "serve-static": "^1.13.1",
    "validator": "^9.3.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
